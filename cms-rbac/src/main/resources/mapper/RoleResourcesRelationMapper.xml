<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.sophia.cms.rbac.mapper.RoleResourcesRelationMapper">

    <select id="deleteByResourceIdIn">
        delete from t_rbac_role_resource_relation where resource_id in
        <foreach collection="resourceIdList" index="index" item="item" open="(" separator="," close=")">
            #{item}
        </foreach>
    </select>
</mapper>
